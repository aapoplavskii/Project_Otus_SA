    +id : integer 
    +name : string 
}

class rack {
    +id : integer 
    +name : string 
}

class cell {
    +id : integer 
    +name : string 
}

class typeDocs {
    +id : integer 
    +name : string 
}

class docWarehouse {
    +id : integer 
