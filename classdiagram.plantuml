    +quantity : integer
}

class order {
    +id : integer 
    +employee_id : integer
    +order_date : date
    +warehouse_id : integer 
}

class recordOrder {
    +id : integer 
    +part_id : integer
    quantity : integer 
}

class purchase {
    +id : integer 
    +employee_id : integer
